<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sushi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sushi">
        <activity android:name=".TermsAndConditions"></activity>
        <activity android:name=".ContactsUs" />
        <activity android:name=".AboutUS" />
        <activity android:name=".MyAccount" />
        <activity android:name=".MyCard" />
        <activity android:name=".Favorites" />
        <activity android:name=".Restaurant" />
        <activity android:name=".Menue" />
        <activity android:name=".Order" />
        <activity android:name=".Home3" />
        <activity android:name=".Home2" />
        <activity android:name=".Register" />
        <activity android:name=".SignIn" />
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>